{
  "name": "@project/backend",
	"license": "MIT",
  "version": "1.0.0",
  "homepage": "./",
  "scripts": {
    "chain": "hardhat node --network hardhat",
    "fork": "hardhat node --no-deploy --network hardhat --fork https://mainnet.infura.io/v3/460f40a260564ac4a4f4b3fffb032dad",
    "checkup": "npm-upgrade && npm install && npm run prettier",
    "compile": "npx hardhat compile",
		"localhost:deploy": "hardhat --network localhost deploy",
		"localhost:export": "hardhat --network localhost export --export ../frontend/src/contracts/abis/localhost.json",
		"rinkeby:deploy": "hardhat --network rinkeby deploy",
		"rinkeby:verify": "hardhat --network rinkeby etherscan-verify --solc-input",
    "rinkeby:export": "hardhat --network rinkeby export --export ../frontend/src/contracts/abis/rinkeby.json",
		"export": "hardhat export --export-all ../frontend/src/contracts/abis/deployments.json",
    "node": "npx hardhat node",
    "prettier": "prettier --write '**/*.{tsx,ts,jsx,js,json}'",
    "pretty-quick": "pretty-quick",
    "test": "hardhat test"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.6",
    "@nomiclabs/hardhat-solhint": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.3.1",
    "@tenderly/hardhat-tenderly": "^1.0.12",
    "@typechain/ethers-v5": "^5.0.0",
    "@typechain/hardhat": "^2.3.0",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "ethereum-waffle": "^3.4.0",
    "hardhat": "^2.6.1",
    "hardhat-abi-exporter": "^2.3.0",
    "hardhat-deploy": "^0.9.3",
    "hardhat-deploy-ethers": "^0.3.0-beta.11",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-preprocessor": "^0.1.4",
    "hardhat-spdx-license-identifier": "^2.0.3",
    "hardhat-typechain": "^0.3.5",
    "hardhat-watcher": "^2.1.1",
    "husky": "^7.0.2",
    "prettier": "^2.4.1",
    "prettier-plugin-solidity": "^1.0.0-beta.18",
    "pretty-quick": "^3.1.1",
    "solidity-coverage": "^0.7.17",
		"typechain": "^4.0.2",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "ethers": "^5.4.6",
    "junk": "^4.0.0",
    "mime-types": "^2.1.32",
    "nft.storage": "^3.2.0"
  }
}
